        "class Point():\n",
        "  def __init__(self, x, y):\n",
        "    self.x = x\n",
        "    self.y = y\n",
        "\n",
        "  def __add__(self, other):\n",
        "    if isinstance(other, tuple):\n",
        "      return Point(self.x + other[0], self.y + other[1])\n",
        "    elif isinstance(other, Point):\n",
        "      return Point(self.x + other.x, self.y + other.y)\n",
        "        \n",
        "\n"
      ],
